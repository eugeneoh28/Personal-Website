  <% include ./partials/header %>
    <div class="container">
      <div class="row">
        <div class="five columns">
          <div class="content">
            <p class="title highlight">My Work.</p>
            <p class="sub-1-title">
              I enjoy working on projects where I can do full-stack development and
              engage with my community.
            </p>
            <img src="/resources/img/totoro2.svg" id="workimg">
          </div>
        </div>
        <div class="seven columns">
          <div class="box">
            <ul class="tabs">
              <li id="titletechnical" href="#technical">Technical Projects</li>
              <li id="titleservice" href="#service">Service Projects</li>
            </ul>
            <div class="projects">
              <div class="project-section" id="technical">
                <div class="project-text">
                  <% Object.keys(data['technical']).forEach(function(k) { %>
                  <a class="project-title" href="/work/tech/<%=k%>"><%= data['technical'][k]['projName'] %>:</a>
                  <p class="project-desc"><%= data['technical'][k]['projDesc'] %></p>
                  <% }); %>
                  <hr>
                </div>
              </div>
              <div class="project-section" id="service">
                <div class="project-text">
                  <% Object.keys(data['service']).forEach(function(k) { %>
                  <a class="project-title" href="/work/service/<%=k%>"><%= data['service'][k]['projName'] %>:</a>
                  <p class="project-desc"><%= data['service'][k]['projDesc'] %></p>
                  <% }); %>
                </div>
              </div>
            </div>

          </div>
        </div>
      </div> 
    </div>
    <script type="text/javascript" src="js/work.js"></script>
  <% include ./partials/footer %>
